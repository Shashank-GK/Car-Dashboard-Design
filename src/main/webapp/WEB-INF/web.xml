<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<!-- Servlet Tags -->
	<!-- Add Car -->
	<servlet>
		<servlet-name>ADD</servlet-name>
		<servlet-class>com.jsp.servlet.AddCarServlet</servlet-class>
	</servlet>
	<!-- Update -->
	<servlet>
		<servlet-name>UPDATE</servlet-name>
		<servlet-class>com.jsp.servlet.UpdateCarServlet</servlet-class>
	</servlet>
	<!-- Delete -->
	<servlet>
		<servlet-name>DELETE</servlet-name>
		<servlet-class>com.jsp.servlet.DeleteCarServlet</servlet-class>
	</servlet>
	<!-- Display -->
	<servlet>
		<servlet-name>DISPLAY</servlet-name>
		<servlet-class>com.jsp.servlet.DisplayCarServlet</servlet-class>
	</servlet>

	<!-- Servlet mapping tags -->
	<!-- ADD -->
	<servlet-mapping>
		<servlet-name>ADD</servlet-name>
		<url-pattern>/add-car</url-pattern>
	</servlet-mapping>
	<!-- UPDATE -->
	<servlet-mapping>
		<servlet-name>UPDATE</servlet-name>
		<url-pattern>/update-car</url-pattern>
	</servlet-mapping>
	<!-- Delete -->
	<servlet-mapping>
		<servlet-name>DELETE</servlet-name>
		<url-pattern>/delete-car</url-pattern>
	</servlet-mapping>
	<!-- Display -->
	<servlet-mapping>
		<servlet-name>DISPLAY</servlet-name>
		<url-pattern>/display-car</url-pattern>
	</servlet-mapping>

</web-app>
